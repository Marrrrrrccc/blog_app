<%- include ("partials/header") %>
<h1>Index page</h1>
<% blogs.forEach(function(blog){%>
<div>
  <h1><%=blog.name%></h1>
  <img src="<%=blog.image%>" alt="" />
  <span><%=blog.date.toDateString()%></span>
  <p><%=blog.body.substring(0,100)%>...</p>
  <a href="/blogs/<%=blog._id %> ">Read more</a>
</div>

<%})%>
<%- include ("partials/footer") %>